<% include header %>
<div class="wrapper">
  <div class="header-nav">
    <div class="logo">
      <h1>Cal Tracker</h1>
    </div>
    <div class="header-links">
      <h2>Calorie Chart</h2>
    </div>
  </div>

  <div class="counter-main calorie-display">
    <div class="day">
      <div class="day-row">
        <div class="meal">
          <h2>Breakfast</h2>
        </div>
        <div class="meal-items">
          <div class="item">
            <h3>Veggie Sausage</h3>
          </div>
          <div class="calories">
            <h3>146</h3>
          </div>
          <div class="item">
            <h3>English Muffin</h3>
          </div>
          <div class="calories">
            <h3>112</h3>
          </div>
        </div>
      </div>
    </div>
    <!--  -->
    <div class="day">
      <div class="day-row">
        <div class="meal">
          <h2>Lunch</h2>
        </div>
        <div class="meal-items">
          <div class="item">
            <h3>Tuna Sammy</h3>
          </div>
          <div class="calories">
            <h3>356</h3>
          </div>
        </div>
      </div>
    </div>
  </div>

</div>


<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/calDisplay.js" charset="utf-8"></script>
